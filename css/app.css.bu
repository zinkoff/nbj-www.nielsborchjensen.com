/* nbj-projects */
.works {
  display: inline-block;
  width: 100%; }

.dots-at-work {
  height: 20px;
  background-image: url(images/dotted_line.gif);
  background-repeat: no-repeat;
  border-bottom: 0.1em dotted; }

.works p {
  float: left; }

.works img {
  width: 333px;
  border: 0;
  margin: 5px; }

.works span {
  border: 0;
  margin: 5px; }

