/* #pageHead	*/
#pageHead {
  padding: 10px 0 30px 0;
  margin: 0 3% 30px 3%;
  background: transparent url(images/dotted_line.gif) 0 bottom repeat-x; }

#pageHead h1 {
  font-size: 1.4em;
  line-height: .7em;
  padding: 0;
  margin-bottom: 0;
  display: block;
  color: #555555; }

#pageHead p {
  margin: 0 !important;
  padding: 10px 0 0 0 !important;
  color: #b2b2b2;
  line-height: 2em;
  /*Ã¦ndret fra 1em*/ }

/* 	Typografi	*/
h2.trompet {
  color: #333333;
  font-size: rem-calc(11);
  line-height: 10.2;
  max-width: 75%;
  font-weight: 500;
  font-style: italic; }

.text-center, .center {
  text-align: center; }

.text-right, .right {
  text-align: right; }

.text-left, .left {
  text-align: left; }
